{
  "compilerOptions": {
    "target": "es2020",
    "moduleResolution": "node",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noEmit": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules"
  ],
}
